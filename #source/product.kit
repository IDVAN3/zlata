<!DOCTYPE html>
<!-- Оболочка документа -->
<html lang="ru">

<!-- Заголовок страницы, контейнер для других важных данных (не отображается) -->

<head>
    <title>Главная</title>
    <!-- Подключение CSS -->
    <link rel="stylesheet" href="css/style.css">
    <link rel="stylesheet" href="libs/jquery.fancybox.min.css">
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
    <link rel="shortcut icon" href="img/icons/favicon.ico">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>
    <!-- ОБОЛОЧКА -->
    <div class="wrapper">
        <div class="content">
            <div class="wrapper-header">
            <!-- @import "header.kit" -->

            <section class="bread product-bread">
                <div class="container">
                    <div class="wrapper-bread-crumbs">
                        <div class="bread-crumbs">
                            <span class="bread-crumbs__line">/</span>
                            <a href="#" class="bread-crumbs__link">Каталог</a>
                            <span class="bread-crumbs__line">/</span>
                            <a href="#" class="bread-crumbs__link">Серьги</a>
                            <span class="bread-crumbs__line">/</span>
                            <a href="#" class="bread-crumbs__link">Серьги из золота</a>
                            <span class="bread-crumbs__line">/</span>
                            <a href="#" class="bread-crumbs__link">Серьги из золота с топазами</a>
                        </div>
                    </div>
                </div>
            </section>

            <section class="section-product">
                <div class="container">
                    <div class="product">
                        <div class="product__slice">
                            <div class="sliderbig">
                                <div class="sliderbig__item">
                                    <div class="wrapper-big">
                                        <a data-fancybox="gallery" href="img/galery/product1big.png">
                                            <img src="img/galery/product1big.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="sliderbig__item">
                                    <div class="wrapper-big">
                                        <a data-fancybox="gallery" href="img/galery/product2big.png">
                                            <img src="img/galery/product2big.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="sliderbig__item">
                                    <div class="wrapper-big">
                                        <a data-fancybox="gallery" href="img/galery/product3big.png">
                                            <img src="img/galery/product3big.png" alt="">
                                        </a>
                                    </div>
                                </div>
                                <div class="sliderbig__item">
                                    <div class="wrapper-big">
                                        <a data-fancybox="gallery" href="img/galery/product4big.png">
                                            <img src="img/galery/product4big.png" alt="">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="slider-product">
                                <div class="product__item">
                                    <div class="wrapper-product">
                                        <img src="img/galery/product1.png" alt="">
                                    </div>
                                </div>
                                <div class="product__item">
                                    <div class="wrapper-product">
                                        <img src="img/galery/product2.png" alt="">
                                    </div>
                                </div>
                                <div class="product__item">
                                    <div class="wrapper-product">
                                        <img src="img/galery/product3.png" alt="">
                                    </div>
                                </div>
                                <div class="product__item">
                                    <div class="wrapper-product">
                                        <img src="img/galery/product4.png" alt="">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="product__slice character">
                            <h2 class="product__title">Серьги из золота с топазами</h2>
                            <div class="art__title product__art">Арт.: Ч0075-45</div>
                            <div class="prise">
                                <div class="old-prise">29 992</div>
                                <div class="new-price">21 695 <span>тенге</span></div>  
                            </div> 
                            <div class="product__buy">
                                <button class="btn">Купить</button>
                                <a href="#">
                                    <div class="icon-circle white1">
                                        <img src="img/icons/new-like.png" alt="" class="new-like">
                                    </div>
                                </a>
                            </div>
                            <div class="characteristics">
                                <h3 class="characteristics__title">Характеристики:</h3>
                            </div>
                            <div class="characteristics__body">
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Артикул:</h5>
                                        <h5 class="characteristics__item">Ч0075-45</h5>
                                    </div>
                                </div>
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Пол:</h5>
                                        <h5 class="characteristics__item">Женские</h5>
                                    </div>
                                </div>
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Металл:</h5>
                                        <h5 class="characteristics__item">Золото 585</h5>
                                    </div>
                                </div>
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Тип изделия:</h5>
                                        <h5 class="characteristics__item">Серьги</h5>
                                    </div>
                                </div>
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Замок сережки:</h5>
                                        <h5 class="characteristics__item">Скоба (булавка)</h5>
                                    </div>
                                </div>
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Артикул:</h5>
                                        <h5 class="characteristics__item">Ч0075-45</h5>
                                    </div>
                                </div>
                                <div class="wrapper-slice">
                                    <div class="characteristics__slice">
                                        <h5 class="characteristics__item">Пол:</h5>
                                        <h5 class="characteristics__item">Женские</h5>
                                    </div>
                                </div>

                                <div class="wrapper-all">
                                    <a href="#" class="link-all">
                                        <h5 class="all-characteristics">Все характеристики</h5>
                                    </a>
                                    <svg class="icon-bottom">
                                        <use xlink:href="img/icons/sprite.svg#bottom"></use>
                                    </svg>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section class="section-fashion">
                <div class="container">
                    <div class="fashion">
                        <h2 class="shop__title">Дополните свой образ</h2>
                        <div class="recently__main fashion__main">
                            <div class="shop__item fashion__item">
                                <a href="#">
                                    <div class="shop__circle">
                                        <img src="img/shop1.png" alt="">
                                        <img src="img/shop-dop.png" alt="" class="additional">
                                    </div>
                                    <div class="art__title">Арт.: Ч0075-45</div>   
                                    <div class="art__description">Золотые серьги с аметистами</div> 
                                </a>                            
                            </div>
                            <div class="shop__item fashion__item">
                                <a href="#">
                                    <div class="shop__circle">
                                        <img src="img/shop2.png" alt="">
                                        <img src="img/shop-dop.png" alt="" class="additional"> 
                                    </div>
                                    <div class="art__title">Арт.: Ч0075-45</div>   
                                    <div class="art__description">Золотые серьги с цитринами </div>
                                </a>  
                            </div>
                            <div class="shop__item fashion__item">
                                <a href="#">
                                    <div class="shop__circle">
                                        <img src="img/shop1.png" alt="">
                                        <img src="img/shop-dop.png" alt="" class="additional">
                                    </div>
                                    <div class="art__title">Арт.: Ч0075-45</div>   
                                    <div class="art__description">Золотые серьги с аметистами</div> 
                                </a>                            
                            </div>
                            <div class="shop__item fashion__item">
                                <a href="#">
                                    <div class="shop__circle">
                                        <img src="img/shop2.png" alt="">
                                        <img src="img/shop-dop.png" alt="" class="additional"> 
                                    </div>
                                    <div class="art__title">Арт.: Ч0075-45</div>   
                                    <div class="art__description">Золотые серьги с цитринами </div>
                                </a>  
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
            
        <!-- @import "footer.kit" -->
   </div>
  
</div>
<!-- попап -->
<div class="popup js-popup" data-id-popup="popup-0">
   <div class="popup__opas"></div>
   <div class="popup__wrapp js-popup__wrapp">
       <span class="popup__close js-popup__close"></span>
       <div class="popup-container">
           <h2>Попап</h2>
       </div>
   </div>
</div>
